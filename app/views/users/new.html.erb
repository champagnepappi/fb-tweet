<% provide(:title, "Signup")%>
<%= form_for(@user) do |f| %>
    <%= f.label :first_name %>
    <%= f.text_field :f_name %>
    
    <%= f.label :last_name %>
    <%= f.text_field :l_name %>

    <%= f.label :email %>
    <%= f.email_field :email %>

    <%= f.label :password %>
    <%= f.password_field :password %>

    <%= f.label :password_confirmation %>
    <%= f.password_field :password_confirmation %>

 <% end %>
