<% provide(:title, "Users")%>
<div class="medium-12">
    <table>
        <thead>
            <tr>
                <th>Name</th>
                <th>Email</th>
            </tr>
        </thead>
        <% @users.each do |user|%>
            <tbody>
                <tr>
                    <td> <%= user.f_name %></td>
                    <td> <%= user.email %></td>
                </tr>
            </tbody>
        <% end %>
    </table>
</div>
