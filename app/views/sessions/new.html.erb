<% provide(:title, 'Login')%>
<div class="medium-6 columns medium-offset-3" >
    <h1>Login Here</h1>
    <div class="callout">
        <%= form_for(:session, url: login_path) do |f| %>
            <%= f.label :email %>
            <%= f.email_field :email %>

            <%= f.label :password %>
            <%= f.password_field :password %>

            <%= f.submit "Submit", class: "button" %>
        <% end %>

    </div>
</div>

